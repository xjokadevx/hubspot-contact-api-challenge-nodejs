{
  "testEnvironment": "node",
  "testMatch": ["**/*.test.js"],
  "transform": {
    "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
  },
  "testPathIgnorePatterns": ["<rootDir>/node_modules/"],
  "collectCoverage": true,
  "coverageDirectory": "coverage",
  "coverageReporters": ["json", "lcov", "text", "clover"]
}
