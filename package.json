{
  "name": "nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prettier:format": "prettier --write .",
    "lint": "eslint . ",
    "lint:fix": "eslint 'src/**/*.js' --fix",
    "format": "npm run prettier:format && npm run lint:fix",
    "test": "rm -rf dist && npm run build && jest",
    "test:dev": "rm -rf dist && npm run build && jest --watchAll --detectOpenHandles",
    "prepare": "husky install",
    "dev": "cross-env DEBUG=development:* NODE_ENV=development nodemon --exec babel-node -L src/index.js",
    "build": "babel src -d dist ",
    "start": "rm -rf node_modules dist && npm i && npm run build && cross-env NODE_ENV=production node dist/index.js",
    "start:local": "rm -rf node_modules dist && npm install && npm run start:dev",
    "start:dev": "cross-env DEBUG=development:* NODE_ENV=dev nodemon src/index.js"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run format",
      "npm run lint"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/node": "^7.25.0",
    "@babel/preset-env": "^7.25.4",
    "@hubspot/api-client": "^11.2.0",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.2.0",
    "helmet": "^7.1.0",
    "inversify": "^6.0.2",
    "inversify-express-utils": "^6.4.6",
    "joi": "^17.13.3",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/express-request-id": "^3.0.0",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "chai": "^5.1.1",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "mocha": "^10.7.3",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
